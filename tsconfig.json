  "compilerOptions": {
    "module": "es6",
    "target": "esnext",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "noImplicitAny": true,
    "jsx": "react",
    "moduleResolution": "node",
    "allowJs": true
  },
  "exclude": [
    "node_modules"
  ]
}